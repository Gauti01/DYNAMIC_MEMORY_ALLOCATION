#include<stdio.h>
void bubblesort(int *A, int n){
    int i,j,temp;
    for(i=0;i<n;i++){
        for(j=0;j<n-1;j++){
            if(A[j]>A[j+1]){
            temp=A[j];
            A[j]=A[j+1];
            A[j+1]=temp;}
        }
    }
}
int main(){
    int i, A[]={12,18,190,34,2,3};
    bubblesort(A,6);
    for(i=0;i<6;i++){
        
        printf("%d\t",A[i]);
    }
    
    
}
